{
    "name": "Recisee",
    "version": "0.0.0.1",
    "description": "Get only what you want out of a recipe page",
    "permissions": ["activeTab"],
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/rc16.png",
        "32": "images/rc32.png",
        "48": "images/rc48.png",
        "128": "images/rc128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["http://*/*"],
        "js": ["jquery-3.5.1.min.js", "strip.js"]
      }
    ],
    "icons": {
      "16": "images/rc16.png",
      "32": "images/rc32.png",
      "48": "images/rc48.png",
      "128": "images/rc128.png"
    },
    "web_accessible_resources": ["jquery-3.5.1.min.js", "clean.js"],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "manifest_version": 2
}